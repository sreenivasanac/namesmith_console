generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["postgresqlExtensions"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Domain_names_basic_info {
  id           Int      @id @default(autoincrement())
  name         String   @unique
  creationDate DateTime
}